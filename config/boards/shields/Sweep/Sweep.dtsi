#include <dt-bindings/zmk/matrix_transform.h>

/ {

    chosen {
        zmk,kscan = &kscan0;
        zmk,matrix_transform = &default_transform;
    };

    default_transform: keymap_transform_0 {
        compatible = "zmk,matrix-transform";
        columns = <32>;
        rows = <1>;
        map = <
        //              0        1        2                            3        4        5
                     RC(0,6)  RC(0,7)  RC(0,8)                      RC(0,25) RC(0,24) RC(0,23)
        //     6        7        8        9                            10       11       12       13
            RC(0,10) RC(0,11) RC(0,12) RC(0,13)                     RC(0,30) RC(0,29) RC(0,28) RC(0,27)
        //                                         14         15
                                                RC(0,16)   RC(0,33)
        >;
    };

};
